<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- The main content view -->

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:background="#44755e"

            android:id="@+id/content_frame1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
            <!-- Add content here -->
            <LinearLayout
                android:background="#44755e"
                android:baselineAligned="false"
                android:orientation="horizontal"
                android:id="@+id/MainBox"


                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_horizontal|top">

                <LinearLayout
                    android:background="#44755e"
                    android:baselineAligned="false"
                    android:id="@+id/Left_sideMainBoxSeeZooAnim"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <LinearLayout
                        android:background="#44755e"
                        android:id="@+id/Left_Side_VerticalBox"
                        android:baselineAligned="false"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2">

                        <ViewFlipper
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="5dp"
                            android:layout_weight="3"
                            android:id="@+id/viewflipper1"
                            android:autoStart="true">
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/tiger3"
                                android:id="@+id/imageview4"/>
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/tiger2"
                                android:id="@+id/imageview5"/>
                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/tiger1"
                                android:id="@+id/imageview6"/>
                        </ViewFlipper>


                        <TextView
                            android:layout_weight="2"
                            android:textStyle="normal"
                            android:textSize="14sp"
                            android:layout_marginRight="5dp"
                            android:padding="10dp"
                            android:background="@drawable/textview_gradient"
                            android:textColor="#fffaffdd"
                            android:layout_marginBottom="5dp"
                            android:layout_marginLeft="5dp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:text="@string/description_of_the_animal"
                            android:gravity="center_horizontal"
                            android:id="@+id/textView"
                            />

                        <LinearLayout

                            android:id="@+id/Button_Containing_Layout"
                            android:baselineAligned="false"
                            android:layout_weight="3.8"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="5dp"
                            android:layout_gravity="center_horizontal">




                            <FrameLayout

                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <LinearLayout
                                    android:background="#44755e"

                                    android:layout_width="match_parent"
                                    android:orientation="horizontal"
                                    android:layout_height="match_parent">


                                    <FrameLayout

                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent">

                                        <ImageButton
                                            android:src="@drawable/dolphin3"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            style="@style/animalspagebtnstyle4"
                                            android:text="@string/check_on_map"
                                            android:paddingBottom="30dp"
                                            android:id="@+id/imageButton2"/>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:text="BASIC INFO"
                                            android:textSize="11sp"
                                            android:paddingTop="35dp"
                                            android:layout_gravity="center_horizontal"
                                            android:textColor="#ffffff"
                                            android:layout_height="wrap_content" />


                                    </FrameLayout>


                                    <FrameLayout

                                        android:layout_width="0dp"
                                        android:layout_weight="1"
                                        android:layout_height="match_parent">

                                        <ImageButton
                                            android:src="@drawable/ic_map"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:paddingBottom="20dp"
                                            style="@style/animalspagebtnstyle4"
                                            android:text="@string/check_on_map"
                                            android:id="@+id/imageButton3"/>

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:text="CHECK ON MAP"
                                            android:textSize="11sp"

                                            android:layout_gravity="center_horizontal"
                                            android:textColor="#ffffff"
                                            android:paddingTop="35dp"
                                            android:layout_height="wrap_content" />


                                    </FrameLayout>









                                </LinearLayout>


                            </FrameLayout>



                        </LinearLayout>


                    </LinearLayout>




                </LinearLayout>

                <LinearLayout
                    android:baselineAligned="false"
                    android:id="@+id/Right_sideMainBox"
                    android:background="#44755e"
                    android:layout_weight="2"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/Right_side_verticalBox"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="1">





                        <FrameLayout
                            android:background="#44755e"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <ImageButton
                                android:layout_marginTop="5dp"

                                android:layout_marginRight="5dp"
                                android:height="200dp"
                                android:background="#ff2b5740"
                                android:paddingLeft="0dp"
                                android:paddingBottom="0dp"
                                android:src="@drawable/ic_gallery"
                                android:layout_marginLeft="0dp"
                                android:paddingRight="60dp"
                                android:text="@string/link_to_gallery"
                                style="@style/animalspagebtnstyle"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/imageButton" />

                            <TextView
                                android:layout_width="match_parent"
                                android:text="GALLERY"
                                android:textSize="11sp"
                                android:textColor="#ffffff"
                                android:layout_marginLeft="45dp"
                                android:layout_marginTop="1dp"
                                android:layout_marginBottom="1dp"
                                android:gravity="center_vertical"
                                android:layout_height="match_parent" />



                        </FrameLayout>


                        <LinearLayout
                            android:background="#ff2b5740"
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_marginBottom="11dp"
                            android:layout_marginTop="5dp"
                            android:layout_height="match_parent">



                            <TextView
                                android:layout_weight="1"
                                android:layout_marginRight="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#44755e"
                                android:textColor="#ffff"
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="@string/gestation_period"
                                android:id="@+id/textView16"
                                android:paddingLeft="5dp"
                                />

                            <TextView
                                android:layout_weight="1"
                                android:layout_marginRight="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#44755e"
                                android:textColor="#ffff"
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="attribute"
                                android:id="@+id/textView26"
                                android:paddingLeft="5dp"
                                />

                            <TextView
                                android:layout_weight="1"
                                android:layout_marginRight="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#44755e"
                                android:textColor="#ffff"
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="@string/mass"
                                android:id="@+id/textView17"
                                android:paddingLeft="5dp" />

                            <TextView
                                android:layout_weight="1"
                                android:layout_marginRight="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#44755e"
                                android:textColor="#ffff"
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="@string/height"
                                android:id="@+id/textView18"
                                android:paddingLeft="5dp" />

                            <TextView
                                android:layout_weight="1"
                                android:layout_marginRight="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#44755e"
                                android:textColor="#ffff"
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="@string/diet"
                                android:id="@+id/textView19"
                                android:paddingLeft="5dp" />

                            <TextView
                                android:layout_weight="1"
                                android:layout_marginRight="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#44755e"
                                android:textColor="#ffff"
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="@string/habitat"
                                android:id="@+id/textView20"
                                android:paddingLeft="5dp" />


                            <TextView
                                android:layout_weight="1"
                                android:layout_marginRight="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="#44755e"
                                android:textColor="#ffff"
                                android:textStyle="bold"
                                android:textAppearance="?android:attr/textAppearanceSmall"
                                android:text="@string/threats"
                                android:id="@+id/textView21"
                                android:paddingLeft="5dp" />



                        </LinearLayout>



                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>



        </FrameLayout>


    </LinearLayout>


    <!-- The navigation drawer -->
    <ListView android:id="@+id/left_drawer"
        android:layout_width="260dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:choiceMode="singleChoice"
        android:divider="#e5e5e5"
        android:dividerHeight="1dp"
        android:background="#d6d6d6"/>
</android.support.v4.widget.DrawerLayout><!-- From: file:/C:/Users/Chinthana/Desktop/INTEGRATED/ZooCeylon2_backup/app/src/main/res/layout/see_zoo_animal.xml -->